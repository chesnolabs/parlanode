{
  "name": "parlameter-front",
  "version": "0.0.1",
  "description": "Parlameter frontend server for serving embeds and the page with cms for managing them",
  "main": "run.js",
  "scripts": {
    "dev": "NODE_ENV=dev_local node_modules/.bin/supervisor run",
    "start": "NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu pm2 start run.js --name=parlanode",
    "test": "mocha tests/api_test.js",
    "cards-build": "run(){ CARD_NAME=$1 NODE_ENV=production node cards/build.js; }; run",
    "cards-dev": "run(){ CARD_NAME=$1 webpack-dev-server --config cards/webpack.config.dev.js --content-base cards --hot --progress --open --inline; }; run",
    "cards-generate": "run(){ mkdir -p cards/$1 && cp -R cards/_empty/* cards/$1; }; run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciZ/parlameter-front.git"
  },
  "author": "FranciZ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FranciZ/parlameter-front/issues"
  },
  "homepage": "https://github.com/FranciZ/parlameter-front#readme",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.1",
    "commander": "2.9.0",
    "cors": "^2.8.3",
    "css-loader": "^0.28.4",
    "date-fns": "^1.28.5",
    "ejs": "^2.5.6",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.3.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "full-icu": "^1.2.0",
    "inquirer": "3.1.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "mongoose": "^4.10.5",
    "node-sass": "^4.5.3",
    "pageres": "4.5.0",
    "parlassets": "danesjenovdan/parlassets",
    "rand-token": "^0.3.0",
    "request": "^2.81.0",
    "require-all": "2.2.0",
    "sass-loader": "^6.0.5",
    "serve-static": "^1.12.3",
    "supertest": "^3.0.0",
    "supervisor": "0.12.0",
    "uglify-js": "3.0.15",
    "vue": "2.3.4",
    "vue-loader": "12.2.1",
    "vue-server-renderer": "2.3.4",
    "vue-template-compiler": "2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webshot": "^0.18.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "plugins": [
      "html",
      "import"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off"
    }
  },
  "babel": {
    "plugins": [
      "lodash",
      "transform-exponentiation-operator"
    ],
    "presets": [
      "es2015"
    ]
  }
}
