  },
  "homepage": "https://github.com/danesjenovdan/parlanode#readme",
  "dependencies": {
    "axios": "0.18.0",
    "babel-core": "6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "commander": "2.15.1",
    "cors": "^2.8.4",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "date-fns": "^1.29.0",
    "ejs": "^2.6.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
