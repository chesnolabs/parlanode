    "webpack": "^4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2",
    "webshot": "^0.18.0"
