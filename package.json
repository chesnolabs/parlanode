{
  "name": "parlameter-front",
  "version": "0.0.1",
  "description": "Parlameter frontend server for serving embeds and the page with cms for managing them",
  "main": "run.js",
  "scripts": {
    "dev": "NODE_ENV=dev_local NODE_ICU_DATA=node_modules/full-icu node_modules/.bin/supervisor run",
    "start": "NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu pm2 start run.js --name=parlanode",
    "test": "mocha tests/api_test.js",
    "cards-build": "run(){ CARD_NAME=$1 NODE_ENV=production node cards/build.js; }; run",
    "cards-dev": "run(){ CARD_NAME=$1 webpack-dev-server --config cards/webpack.config.dev.js --content-base cards --hot --progress --open --inline; }; run",
    "cards-generate": "run(){ mkdir -p cards/$1 && cp -R cards/_empty/* cards/$1; }; run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciZ/parlameter-front.git"
  },
  "author": "FranciZ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FranciZ/parlameter-front/issues"
  },
  "homepage": "https://github.com/FranciZ/parlameter-front#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.2",
    "commander": "2.12.0",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "date-fns": "^1.29.0",
    "ejs": "^2.5.7",
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.0.0-beta.0",
    "express": "~4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "full-icu": "^1.2.0",
    "inquirer": "3.1.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "mongoose": "^4.13.5",
    "node-sass": "^4.7.2",
    "pageres": "4.5.1",
    "parlassets": "danesjenovdan/parlassets",
    "rand-token": "^0.3.0",
    "request": "^2.83.0",
    "require-all": "2.2.0",
    "sass-loader": "^6.0.6",
    "serve-static": "^1.13.1",
    "supertest": "^3.0.0",
    "supervisor": "0.12.0",
    "uglify-js": "3.2.0",
    "vue": "2.5.8",
    "vue-loader": "13.5.0",
    "vue-server-renderer": "2.5.8",
    "vue-template-compiler": "2.5.8",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "vue-infinite-scroll": "^2.0.2",
    "webshot": "^0.18.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "globals": {
      "measure": false,
      "$": false
    },
    "extends": "airbnb-base",
    "plugins": [
      "vue",
      "import"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off"
    }
  },
  "babel": {
    "plugins": [
      "lodash",
      "transform-object-rest-spread"
    ],
    "presets": [
      "env"
    ]
  }
}
