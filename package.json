{
  "name": "parlameter-front",
  "version": "0.0.1",
  "description": "Parlameter frontend server for serving embeds and the page with cms for managing them",
  "main": "run.js",
  "scripts": {
    "dev": "NODE_ENV=development supervisor run",
    "dev-local": "NODE_ENV=dev_local supervisor run",
    "start": "NODE_ENV=production pm2 start run.js --name=parlanode",
    "test": "mocha tests/api_test.js",
    "cards-build": "run(){ CARD_NAME=$1 NODE_ENV=production node cards/build.js; }; run",
    "cards-dev": "run(){ CARD_NAME=$1 webpack-dev-server --config cards/webpack.config.dev.js --content-base cards --hot --progress --open; }; run",
    "cards-generate": "run(){ mkdir -p cards/$1 && cp -R cards/_empty/* cards/$1; }; run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciZ/parlameter-front.git"
  },
  "author": "FranciZ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FranciZ/parlameter-front/issues"
  },
  "homepage": "https://github.com/FranciZ/parlameter-front#readme",
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "bluebird": "^3.3.1",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.1",
    "cheerio": "^0.22.0",
    "cors": "^2.7.1",
    "css-loader": "^0.26.1",
    "ejs": "^2.4.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "express-validator": "^2.20.8",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.2",
    "mongoose": "^4.4.4",
    "node-sass": "^4.5.2",
    "pageres": "4.3.0",
    "parlassets": "git+ssh://git@github.com:muki/parlassets.git",
    "pg": "^6.0.1",
    "pg-hstore": "^2.3.2",
    "rand-token": "^0.2.1",
    "request": "^2.69.0",
    "sass-loader": "^6.0.3",
    "sequelize": "^3.25.0",
    "serve-static": "^1.10.2",
    "supertest": "^2.0.1",
    "uglify-js": "2.8.20",
    "vue": "2.2.6",
    "vue-loader": "11.3.4",
    "vue-server-renderer": "2.2.6",
    "vue-template-compiler": "2.2.6",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webshot": "^0.18.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base"
  },
  "babel": {
    "plugins": [
      "lodash"
    ],
    "presets": [
      "es2016",
      "es2015"
    ]
  }
}
